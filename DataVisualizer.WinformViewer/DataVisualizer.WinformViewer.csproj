<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFramework>net10.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <Title>Data Visualizer</Title>
    <Version>0.1.0</Version>
    <Authors>cplkimth</Authors>
    <Description>Data Visualizer</Description>
    <PackageProjectUrl>https://github.com/cplkimth/DataVisualizer</PackageProjectUrl>
    <RepositoryUrl>https://github.com/cplkimth/DataVisualizer</RepositoryUrl>
    <PackageTags>Data Visualizer</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="10.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
    <PackageReference Include="DevExpress.Win.Design" Version="25.1.6" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DataVisualizer.Contract\DataVisualizer.Contract.csproj" />
  </ItemGroup>
</Project>